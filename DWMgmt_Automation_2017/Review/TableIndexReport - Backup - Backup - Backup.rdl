  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="TableName">
          <DataField>TableName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="IndexID">
          <DataField>IndexID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="IndexName">
          <DataField>IndexName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="IndexType">
          <DataField>IndexType</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>dhdcdtw_dev</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>select tbl.name as TableName
, idx.index_id as IndexID
, idx.name as IndexName
, idx.type_desc as IndexType
from sys.tables as tbl
join sys.indexes as idx
on idx.object_id = tbl.object_id
where idx.index_id &gt; 0</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
