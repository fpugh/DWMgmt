    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="BaseSessionStats">
      <Query>
        <DataSourceName>SystemProcesses</DataSourceName>
        <CommandText>SELECT ess.login_name, econ.session_id
, econ.num_reads, econ.num_writes
, count(tst.transaction_id) as Active_Transactions
, CASE WHEN econ.last_read &lt; econ.last_write THEN datediff(SS, econ.connect_time, econ.last_write)/60.00 
	ELSE datediff(SS, econ.connect_time, econ.last_read)/60.00  END AS Elapsed_Active_Session
, CASE WHEN econ.last_read &lt; econ.last_write THEN datediff(SS, econ.last_write, getdate())/60.00 
	ELSE datediff(SS, econ.last_read, getdate())/60.00  END AS Elapsed_Dormant_Session
FROM sys.dm_exec_connections AS econ
LEFT JOIN sys.dm_exec_sessions AS ess
ON ess.session_id = econ.session_id
LEFT JOIN sys.dm_tran_session_transactions AS tst
ON tst.session_id = ess.session_id
GROUP BY ess.login_name, econ.session_id
, ess.login_time, econ.connect_time
, econ.num_reads, econ.num_writes
, econ.last_read, econ.last_write</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="login_name">
          <DataField>login_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="session_id">
          <DataField>session_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="num_reads">
          <DataField>num_reads</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="num_writes">
          <DataField>num_writes</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Active_Transactions">
          <DataField>Active_Transactions</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Elapsed_Active_Session">
          <DataField>Elapsed_Active_Session</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Elapsed_Dormant_Session">
          <DataField>Elapsed_Dormant_Session</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
