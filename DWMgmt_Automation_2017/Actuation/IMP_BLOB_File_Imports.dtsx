#region Namespaces
using System;
using System.Data;
using System.IO;
using System.Text;
using System.Linq;
using System.Security.Principal;
using System.Security.AccessControl; 
using Microsoft.SqlServer.Dts.Pipeline.Wrapper;
using Microsoft.SqlServer.Dts.Runtime.Wrapper;
using System.Windows.Forms;
#endregion


[Microsoft.SqlServer.Dts.Pipeline.SSISScriptComponentEntryPointAttribute]
public class ScriptMain : UserComponent
{
    private string FilePath;

    public override void AcquireConnections(object Transaction)
    {
        IDTSConnectionManager100 connMgr = this.Connections.FileSource;
        FilePath = (string)connMgr.AcquireConnection(null);
    }


    public override void CreateNewOutputRows()
    {
        
        string FileEncoding;
        string FileAuthor;
        int CodePage;

        var defaultEncodingIfNoBom = Encoding.ASCII;

        try
        {
            FileAuthor = System.IO.File.GetAccessControl(FilePath).GetOwner(typeof(System.Security.Principal.NTAccount)).ToString();
        }

        catch 
        {
            FileAuthor = "Unknown";
        }
        

        using (var FileReader = new StreamReader(FilePath, defaultEncodingIfNoBom, true))
        {
            FileEncoding = FileReader.CurrentEncoding.WebName;
            CodePage = FileReader.CurrentEncoding.CodePage;
        }
                    
        
        //String FileEncoding = Encoding.GetEncoding(FileContent).ToString();
        FileInfo fi = new FileInfo(FilePath);
        DirectoryInfo di = new DirectoryInfo(FilePath);

        Output0Buffer.AddRow();
        Output0Buffer.FileAuthor = FileAuthor.ToString();
        Output0Buffer.FileType = fi.Extension.ToString();
        Output0Buffer.FileSize = fi.Length.ToString();
        Output0Buffer.FileCreated = fi.CreationTime.ToString();
        Output0Buffer.FileModified = fi.LastWriteTime.ToString();
        Output0Buffer.IsReadOnly = fi.IsReadOnly.ToString();
        Output0Buffer.FileName = fi.Name.ToString();
        Output0Buffer.Encoding = FileEncoding;
        Output0Buffer.CodePage = CodePage;
        Output0Buffer.FilePath = FilePath.ToString();
    }

}
]]></arrayElement>
