    <DTS:Variable
      DTS:CreationName=""
      DTS:DTSID="{76F1804F-8078-47E4-A347-50209C546A25}"
      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;&#xA;SELECT DISTINCT DB_ID() as Database_ID, scm.Schema_ID, obj.Object_ID, idxo.Index_ID, CAST(@@SERVERNAME AS NVARCHAR)  AS Server_Name&#xA;, CAST(DB_NAME() AS NVARCHAR) AS Database_Name, scm.name as Schema_Name, obj.name as Object_Name, ISNULL(idxo.name, obj.name) as Index_Name, idxo.type, idxs.Index_Type_Desc, CAST('&quot;+ @[$Package::ScanMode] +&quot;' as NVARCHAR) as Scan_Mode, idxs.Partition_Number&#xA;, idxs.Alloc_Unit_Type_Desc, idxs.Index_depth, idxs.Index_level&#xA;, ISNULL(idxs.avg_fragmentation_in_percent,0) AS avg_fragmentation_in_percent&#xA;, ISNULL(idxs.fragment_count,0) AS fragment_count&#xA;, ISNULL(idxs.avg_fragment_Size_in_pages,0) AS avg_fragment_Size_in_pages&#xA;, idxs.page_count&#xA;, ISNULL(idxs.avg_page_space_used_in_percent,0) AS avg_page_space_used_in_percent&#xA;, ISNULL(idxs.record_count,0) AS record_count&#xA;, ISNULL(idxs.min_record_Size_in_bytes,0) AS min_record_Size_in_bytes&#xA;, ISNULL(idxs.max_record_Size_in_bytes,0) AS max_record_Size_in_bytes&#xA;, ISNULL(idxs.avg_record_Size_in_bytes,0) AS avg_record_Size_in_bytes&#xA;, ISNULL(idxs.forwarded_record_count,0) AS forwarded_record_count&#xA;, ISNULL(idxs.compressed_page_count,0) AS  compressed_page_count&#xA;FROM sys.dm_db_Index_physical_stats(db_ID(), NULL, NULL, NULL,'&quot;+ @[$Package::ScanMode] +&quot;') as idxs&#xA;JOIN sys.all_objects AS obj&#xA;ON idxs.Database_ID = db_ID()&#xA;AND idxs.Object_ID = obj.Object_ID&#xA;JOIN sys.schemas as scm&#xA;ON scm.Schema_ID = obj.Schema_ID&#xA;LEFT JOIN sys.indexes as idxo&#xA;ON idxo.Object_ID = obj.Object_ID&#xA;AND idxo.Index_ID = idxs.Index_ID&#xA;LEFT JOIN sys.Index_columns as idxc&#xA;ON idxc.Object_ID = obj.Object_ID&#xA;AND idxc.Index_ID = idxs.Index_ID&#xA;LEFT JOIN sys.all_columns as col&#xA;ON col.Object_ID = obj.Object_ID&#xA;AND col.Column_ID = ISNULL(idxc.Column_ID, col.Column_ID)&#xA;WHERE ('&quot;+ @[$Package::NamePart] +&quot;' = 'ALL'&#xA;OR CHARINDEX('&quot;+ @[$Package::NamePart] +&quot;', DB_NAME()+'.'+scm.name+'.'+obj.name) &gt; 0)&#xA;&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="SQL_Index_Metric_Capture">
      <DTS:VariableValue
        DTS:DataType="8">
SELECT DISTINCT DB_ID() as Database_ID, scm.Schema_ID, obj.Object_ID, idxo.Index_ID, CAST(@@SERVERNAME AS NVARCHAR)  AS Server_Name
, CAST(DB_NAME() AS NVARCHAR) AS Database_Name, scm.name as Schema_Name, obj.name as Object_Name, ISNULL(idxo.name, obj.name) as Index_Name, idxo.type, idxs.Index_Type_Desc, CAST('Sampled' as NVARCHAR) as Scan_Mode, idxs.Partition_Number
, idxs.Alloc_Unit_Type_Desc, idxs.Index_depth, idxs.Index_level
, ISNULL(idxs.avg_fragmentation_in_percent,0) AS avg_fragmentation_in_percent
, ISNULL(idxs.fragment_count,0) AS fragment_count
, ISNULL(idxs.avg_fragment_Size_in_pages,0) AS avg_fragment_Size_in_pages
, idxs.page_count
, ISNULL(idxs.avg_page_space_used_in_percent,0) AS avg_page_space_used_in_percent
, ISNULL(idxs.record_count,0) AS record_count
, ISNULL(idxs.min_record_Size_in_bytes,0) AS min_record_Size_in_bytes
, ISNULL(idxs.max_record_Size_in_bytes,0) AS max_record_Size_in_bytes
, ISNULL(idxs.avg_record_Size_in_bytes,0) AS avg_record_Size_in_bytes
, ISNULL(idxs.forwarded_record_count,0) AS forwarded_record_count
, ISNULL(idxs.compressed_page_count,0) AS  compressed_page_count
FROM sys.dm_db_Index_physical_stats(db_ID(), NULL, NULL, NULL,'Sampled') as idxs
JOIN sys.all_objects AS obj
ON idxs.Database_ID = db_ID()
AND idxs.Object_ID = obj.Object_ID
JOIN sys.schemas as scm
ON scm.Schema_ID = obj.Schema_ID
LEFT JOIN sys.indexes as idxo
ON idxo.Object_ID = obj.Object_ID
AND idxo.Index_ID = idxs.Index_ID
LEFT JOIN sys.Index_columns as idxc
ON idxc.Object_ID = obj.Object_ID
AND idxc.Index_ID = idxs.Index_ID
LEFT JOIN sys.all_columns as col
ON col.Object_ID = obj.Object_ID
AND col.Column_ID = ISNULL(idxc.Column_ID, col.Column_ID)
WHERE ('ALL' = 'ALL'
OR CHARINDEX('ALL', DB_NAME()+'.'+scm.name+'.'+obj.name) &gt; 0)
</DTS:VariableValue>
    </DTS:Variable>
