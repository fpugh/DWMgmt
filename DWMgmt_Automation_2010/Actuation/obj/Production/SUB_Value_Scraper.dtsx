      DTS:ObjectName="SQL_Value_Hash_Capture_Statement">
      <DTS:Property
        DTS:DataType="8"
        DTS:Name="ParameterValue">SELECT 1 AS TBL_ID , CAST('nvarchar' as nvarchar) as Column_Type , CASE WHEN 1 = 1 THEN convert(NVARCHAR(4000), REG_Server_Name) ELSE rtrim(ltrim(convert(NVARCHAR(4000), REG_Server_Name))) COLLATE Latin1_General_CS_AS  	END AS Column_Value , CASE WHEN CHARINDEX('CHAR','nvarchar') &gt; 0 AND (CHARINDEX(CHAR(9), REG_Server_Name) &gt; 0 OR CHARINDEX(CHAR(32), REG_Server_Name) &gt; 0 OR LEN(REG_Server_Name) &gt; 256) THEN 1  	ELSE 0 END AS String_Flag , COUNT_BIG(*) as Value_Count, 1 as Batch_ID  FROM DWMgmt.CAT.REG_0100_Server_Registry WITH(NOLOCK) GROUP BY REG_Server_Name</DTS:Property>
    </DTS:PackageParameter>
